import java.util.Scanner;
public class Main {
  public static void main(String[] args) {
  
 
	Scanner leia = new Scanner(System.in); 
	int primeironumero = 0;
	int segundonumero = 0;
	System.out.print("digite o um numero ");
	primeironumero = leia.nextInt();
	System.out.print("digite outro numero ");
	segundonumero = leia.nextInt();
	
	if(primeironumero > segundonumero){
	    System.out.print("o numero " + primeironumero + " é maior que o " + segundonumero);
	}
    else if (primeironumero > segundonumero){
        System.out.print("o numero " + segundonumero + " é maior que o " + primeironumero);
}
}      
}